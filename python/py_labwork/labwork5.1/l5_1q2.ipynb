    "class Counter:\n",
    "    def __init__(self, n):\n",
    "        self.n = n\n",
    "\n",
    "    def counter1(self):\n",
    "        for i in range(self.n, 0, -1):\n",
    "            print(i)\n",
    "\n",
    "n = 10\n",
    "o1 = Counter(n)\n",
    "o1.counter1()\n"
   ]
