   "source": [
    "def factorial(n):\n",
    "    if n == 1:  \n",
    "        return 1\n",
    "    return n * factorial(n - 1)  \n",
    "\n",
    "user_input = int(input(\"Enter number To do factorial :\"))\n",
    "print(factorial(user_input))"
   ]
