    "def fibonacci(n):\n",
    "    if n <= 0:  \n",
    "        return 0\n",
    "    elif n == 1:\n",
    "        return 1\n",
    "    return fibonacci(n - 1) + fibonacci(n - 2) \n",
    "\n",
    "user_input = int(input(\"Enter number To do fibonacci :\"))\n",
    "print(fibonacci(user_input))"
   ]
