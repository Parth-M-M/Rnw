    "def square(*num):\n",
    "    my_list = []\n",
    "    for i in num:\n",
    "        sqr = i * i\n",
    "        my_list.append(sqr)\n",
    "    print(my_list)\n",
    "\n",
    "user_input = input(\"Enter numbers separated by commas: \")\n",
    "my_num = [int(x) for x in user_input.split(\",\")] \n",
    "\n",
    "square(*my_num) \n"
   ]
