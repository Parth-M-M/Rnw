   "source": [
    "def factorial(num):\n",
    "    fac = 1\n",
    "    for i in range(num, 0, -1):\n",
    "        fac *= i\n",
    "    return fac\n",
    "\n",
    "user_input = int(input(\"Enter number: \"))\n",
    "print(factorial(user_input))\n"
   ]
