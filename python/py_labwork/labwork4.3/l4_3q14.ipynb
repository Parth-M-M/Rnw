    "people = [\n",
    "    {\"name\": \"Alice\", \"id\": 30},\n",
    "    {\"name\": \"Bob\", \"id\": 25},\n",
    "    {\"name\": \"Charlie\", \"id\": 35}\n",
    "]\n",
    "\n",
    "sorted_people = sorted(people, key=lambda person: person['id'])\n",
    "\n",
    "print(\"Sorted by age:\")\n",
    "for person in sorted_people:\n",
    "    print(person)"
